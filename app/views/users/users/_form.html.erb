<%= form_with model: user, url: url, data: { turbo_frame: 'user' } do |form| %>
  <div>
    <div>
      <%= form.label :first_name %>
      <%= form.text_field :first_name %>
    </div>
    <div>
      <%= form.label :last_name %>
      <%= form.text_field :last_name %>
    </div>
    <div>
      <%= form.label :email %>
      <%= form.text_field :email %>
    </div>
    <div>
      <%= form.label :password %>
      <%= form.password_field :password %>
    </div>
    <div>
      <%= form.label :role %>
      <%= form.collection_select :role, Role.all, :id, :name %>
    </div>
    <div>
      <%= form.submit %>
    </div>
  </div>
<% end %>
